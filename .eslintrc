{
  "extends": ["plugin:vue/base", "eslint:recommended", "@vue/typescript"],
  "rules": {
    "semi": ["error", "never"],
    "comma-dangle": [
      "error",
      {
        "arrays": "always-multiline",
        "objects": "always-multiline",
        "imports": "always-multiline",
        "exports": "always-multiline",
        "functions": "never"
      }
    ]
  }
}
